<html>
	<head>
		<title>小丑网登陆</title>
		<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
		<script type="text/javascript" src="/javascripts/jquery.js" charset="utf-8"></script>
	    <script type="text/javascript" src="/javascripts/bootstrap.js" charset="utf-8"></script>
	    <script type="text/javascript" src="/javascripts/navbar.js" charset="utf-8"></script>
		<link rel="Shortcut Icon" href="/images/icon/icon.ico">
	</head>
	<body>
							<div>
	                                <div class="modal-dialog modal-sm" role="document">
	                                    <div class="modal-content">
	                                        <div class="modal-header">
	                                    		<h4 class="modal-title">管理员登陆</h4>
	                                        </div>
	
	                                        <div class="modal-body">
	                                              <form class="form-horizontal ng-pristine ng-valid">
	                                                  <div class="form-group">
	                                                      <label for="userName" class="col-xs-2 control-label text-primary glyphicon glyphicon-user"></label>
	                                                      <div class="col-xs-10">
	                                                          <input type="email" class="form-control" id="userName" placeholder="邮箱账号">
	                                                      </div>
	
	                                                  </div>
	                                                  <div class="form-group">
	                                                      <label for="userPassword" class="col-xs-2 control-label text-primary glyphicon glyphicon-lock"></label>
	                                                      <div class="col-xs-10">
	                                                          <input type="password" class="form-control" id="userPassword" placeholder="密码">
	                                                      </div>
	
	                                                  </div>
	                                              </form>
	                                        </div>
	                                        <div class="modal-footer">
	                                            <button class="btn btn-default" data-dismiss="modal">关闭</button>
	                                            <button class="btn btn-primary logIn">登陆</button>
	                                        </div>
	                                    </div>
	                                </div>
	                            </div>
	</body>
</html>